<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/startFragment">
    <fragment
        android:id="@+id/startFragment"
        android:name="com.googletutorial.wuerfelabziehapp.screens.start.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_diceFragment"
            app:destination="@id/diceFragment2" />
        <action
            android:id="@+id/action_startFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
    <fragment
        android:id="@+id/diceFragment2"
        android:name="com.googletutorial.wuerfelabziehapp.screens.dice.DiceFragment"
        android:label="fragment_dice"
        tools:layout="@layout/fragment_dice" />
    <fragment
        android:id="@+id/listFragment"
        android:name="com.googletutorial.wuerfelabziehapp.screens.list.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" />
</navigation>